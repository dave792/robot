void look_nuetral(int t)
{
   // Pause for half second
  display.clearDisplay();  // Clear the buffer
// Displays EYES_FRONT on the screen
  display.drawBitmap(0, 0, eye_nuetral, 128, 64, 1);
  display.display();
  delay(t);
}
